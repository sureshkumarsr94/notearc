{
    "family": "notearc",
    "networkMode": "bridge",
    "containerDefinitions": [
        {
            "name": "notearc",
            "image": "IMAGE_PLACEHOLDER",
            "essential": true,
            "memory": 512,
            "cpu": 256,
            "portMappings": [
                {
                    "containerPort": 3000,
                    "hostPort": 80,
                    "protocol": "tcp"
                }
            ],
            "environment": [
                {
                    "name": "NODE_ENV",
                    "value": "production"
                },
                {
                    "name": "PORT",
                    "value": "3000"
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/notearc",
                    "awslogs-region": "ap-south-1",
                    "awslogs-stream-prefix": "ecs",
                    "awslogs-create-group": "true"
                }
            },
            "healthCheck": {
                "command": [
                    "CMD-SHELL",
                    "wget -q --spider http://localhost:3000 || exit 1"
                ],
                "interval": 30,
                "timeout": 5,
                "retries": 3,
                "startPeriod": 60
            }
        }
    ],
    "requiresCompatibilities": [
        "EC2"
    ],
    "executionRoleArn": "arn:aws:iam::602310134850:role/ecsTaskExecutionRole"
}